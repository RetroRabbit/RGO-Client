    "table",
    "react",
    "table",
    "angular",
    "angular-component",
    "react",
    "react-component",
    "reactjs",
    "vue",
    "vuejs"
  ],
  "jspm": {
    "main": "jspm-main"
  },
  "author": "Niall Crosby <niall.crosby@ag-grid.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie >= 0",
    "not ie_mob >= 0",
    "not blackberry > 0"
